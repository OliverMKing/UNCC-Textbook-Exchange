<h1>Reports</h1>

<%= form_for :report, url: reports_path do |f| %>

<p>
    <%= f.label :name %> <br>
    <%= f.text_field :name %> 
</p>

<p>
    <%= f.label :email %> <br>
    <%= f.text_field :email %> 
</p>

<p>
    <%= f.label :reason %> <br>
    <%= f.text_area :reason %> 
</p>

<p>
    <%= f.submit %>
</p>

<% end %>